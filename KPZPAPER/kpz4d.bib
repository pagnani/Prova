@article{Cook-Derrida1990,
  author={J Cook and B Derrida},
  title={Directed polymers in a random medium: 1/d expansion and the n-tree approximation},
  journal={Journal of Physics A: Mathematical and General},
  volume={23},
  number={9},
  pages={1523},
  url={http://stacks.iop.org/0305-4470/23/i=9/a=017},
  year={1990},
  abstract={The authors develop a method which allows one to obtain 1/d expansions for the problem of directed polymers in a random medium. Using this method, they expand thermal properties (such as the free energy, the specific heat), the overlaps and the fluctuations of the transverse displacement. Their results are consistent with the existence of a finite upper critical dimension above which the low-temperature phase is mean field like, i.e. characterised by a broad distribution of overlaps (broken replica symmetry) and transverse fluctuations which scale as the length of the polymer.}
}
	
